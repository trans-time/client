{{#each timelineItems as |timelineItem|}}
  {{#if timelineItem.panels.firstObject.srcset}}
    {{timeline-item-nav/slideshow/modal-nav/timeline-item
      timelineItem=timelineItem
      activePanels=activePanels
      scrollLeft=scrollLeft
      style=style
      sizes="100vw"
      scrollVertical=(action "scrollVertical")
      slidePanels=(action "slidePanels")
      openModalNav=(action "openModalNav")
    }}
  {{/if}}
{{/each}}
