{{#each posts as |post|}}
  {{#each post.images as |image|}}
    {{post-nav/slideshow/image
      image=image
      navState=navState
    }}
  {{/each}}
{{/each}}

<div class="post-nav-slideshow-pointer-container">
  {{#each-in pointers as |direction icon|}}
    {{post-nav/slideshow/pointer
      direction=direction
      icon=icon
    }}
  {{/each-in}}
</div>
