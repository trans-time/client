{{#each errors as |error|}}
  <li class="paper-toaster-error-item">
    <h1 class="paper-toaster-error-title">
      {{#if (t-exists error.title)}}
        {{t error.title}}
      {{else}}
        {{error.title}}
      {{/if}}
    </h1>
    <div class="paper-toaster-error-detail">
      {{#if (t-exists error.detail)}}
        {{t error.detail}}
      {{else}}
        {{error.detail}}
      {{/if}}
    </div>
  </li>
{{/each}}
